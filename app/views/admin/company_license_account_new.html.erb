<div class="admin_index_header">
  <h1>New Company License Account</h1>
</div>
<div class="half">
  <div>
    <%= simple_form_for(@company_license_account, url: admin_company_license_accounts_path) do |f| %>
      <% if @company_license_account.errors.any? %>
        <div class="errors-container">
          <ul>
            <% @company_license_account.errors.full_messages.each do |message| %>
              <li><%= message %></li>
            <% end %>
          </ul>
        </div>
      <% end %>
      <%= f.input :company_license_id, label: "company_license_id"%>
      <%= f.input :user_id, label: "user_id"%>
      <%= f.input :active, label: "active"%>
      <%= f.input :access_permission, label: "Access Permission"%>
      <%= f.submit 'Create Company License Account', class: 'btn btn-primary',  id: 'booking-btn' %>
    <% end %>
  </div>
  <div class="pathfix-container">
    <h5>Or upload via CSV</h5>
    <button class="btn btn-success">Upload CSV</button>
  </div>
</div>
